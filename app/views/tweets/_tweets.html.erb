<% tweets.each do |tweet| %>
  <div class="tweet">
    <div class="text">
      <%= tweet.name %>
    </div>
    <div class="image">
      <%= link_to image_tag(tweet.image) , tweet_path(tweet.id), method: :get %>
    </div>
    <%= render partial: 'likes/like', locals: { tweet: tweet, like: @like} %>
  </div>
<% end %>